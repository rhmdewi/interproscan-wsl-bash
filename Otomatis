-i /mnt/c/Pisang/fixed/split_part_1_fixed.fasta \
-f tsv \
-o /mnt/c/Pisang/fixed/split_part_1_interpro.tsv \
--disable-precalc -appl Pfam --cpu 1 > interproscan_debug.logÂ 2>&1
